    <ControlTemplate x:Key="ATBTextBox" TargetType="{x:Type TextBoxBase}">
        <Border Height="25" Background="{DynamicResource CheckboxBackground}" BorderBrush="{DynamicResource CheckboxBorder}" x:Name="Bd" BorderThickness="1" CornerRadius="5">
