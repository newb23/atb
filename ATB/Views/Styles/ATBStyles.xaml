                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>

                <TextBlock Padding="5,0" Grid.Column="0" Text="{TemplateBinding Tag}" Foreground="Gray" VerticalAlignment="Center" />
