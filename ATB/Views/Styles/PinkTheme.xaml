<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="ButtonFace"
		StartPoint="0.51,0" EndPoint="0.51,0.99"
		SpreadMethod="Pad" ColorInterpolationMode="SRgbLinearInterpolation">
        <GradientStop Color="#FFDA56F7" Offset="0.51" />
        <GradientStop Color="#FFD924E2" Offset="1" />
        <GradientStop Color="#FFD924E2" Offset="0" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ButtonMouseOver"
		ColorInterpolationMode="SRgbLinearInterpolation" EndPoint="0.51,0.99" SpreadMethod="Pad" StartPoint="0.51,0">
        <GradientStop Color="#FFE99CF5" Offset="0.51" />
        <GradientStop Color="#FFE073F1" Offset="1" />
        <GradientStop Color="#FFE073F1" Offset="0" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TabUnselectedBackground"
		StartPoint="0.51,0" EndPoint="0.51,0.99"
		SpreadMethod="Pad" ColorInterpolationMode="SRgbLinearInterpolation">
        <GradientStop Color="#FF282B30" Offset="0.51" />
        <GradientStop Color="#FF1F2125" Offset="1" />
        <GradientStop Color="#FF1F2125" Offset="0" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TabSelectedBackground"
		StartPoint="0.51,0" EndPoint="0.51,0.99"
		SpreadMethod="Pad" ColorInterpolationMode="SRgbLinearInterpolation">
        <GradientStop Color="#FFC227C9" Offset="0.025" />
        <GradientStop Color="#FF0A0A0A" Offset="1" />
        <GradientStop Color="#FF4B4B4B" Offset="0" />
        <GradientStop Color="#FFB73BDC" Offset="0.992" />
        <GradientStop Color="#FFF75CE9" Offset="0.527" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TabSelectedBorderColor" Color="#FFA7F09C" />
</ResourceDictionary>